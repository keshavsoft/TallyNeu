<PARMVAR>SVFromDate:Date:$$Date:&quot;11-07-2024&quot;</PARMVAR>
            <PARMVAR>SVToDate:Date:$$Date:&quot;11-07-2024&quot;</PARMVAR>

          </COLLECTION>

