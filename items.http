          <COLLECTION NAME="KS_datacoll1" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
            <TYPE>vouchers:vouchertype</TYPE>
            <CHILDOF>$$VchTypeSales</CHILDOF>
